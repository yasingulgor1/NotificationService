# Application
APP_ENV=development
LOG_LEVEL=debug

# Server
SERVER_PORT=8080

# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/notifications?sslmode=disable

# Redis
REDIS_URL=redis://localhost:6379/0

# Webhook Provider (get your URL from https://webhook.site)
WEBHOOK_URL=https://webhook.site/your-uuid-here

# Worker Configuration
WORKER_COUNT_SMS=5
WORKER_COUNT_EMAIL=5
WORKER_COUNT_PUSH=5

# Rate Limiting
RATE_LIMIT_PER_CHANNEL=100

# Retry Configuration
MAX_RETRY_COUNT=5
RETRY_BASE_DELAY=1s
